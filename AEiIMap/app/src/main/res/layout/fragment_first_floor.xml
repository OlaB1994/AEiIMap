                        android:id="@+id/fragment_first_floor_119a"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/holo_red_light" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_119b"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_118"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/holo_red_light" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_117"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_116a"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
