                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white" />


                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/plan_dimen_hall"
                    android:layout_gravity="center"
                    android:gravity="center|left"
                    android:orientation="horizontal"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/plan_dimen_center"
                    android:layout_gravity="center"
                    android:gravity="center|left"
                    android:orientation="horizontal"
                    android:weightSum="15">


                    <ImageButton
                        android:id="@+id/fragment_first_floor_125"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/holo_red_light" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_124"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@android:color/white" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_123"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/holo_red_light" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_122"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@android:color/white" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_121"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@android:color/holo_red_light" />

                    <ImageButton
                        android:id="@+id/fragment_first_floor_120"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white" />

